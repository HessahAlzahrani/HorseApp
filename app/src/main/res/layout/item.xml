<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_margin="10dp">


    <TextView
        android:id="@+id/item_name_id"
        style="@style/Widget.MaterialComponents.TextView"
        android:layout_width="111dp"
        android:layout_height="43dp"
        android:fontFamily="sans-serif"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3_id"
        app:layout_constraintVertical_bias="1.0"
        tools:layout_editor_absoluteX="247dp" />

    <TextView
        android:id="@+id/item_type_id"
        style="@style/Widget.MaterialComponents.TextView"
        android:layout_width="116dp"
        android:layout_height="43dp"
        android:layout_below="@+id/item_name_id"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="96dp"
        android:fontFamily="sans-serif-medium"
        android:textAlignment="center"
        app:layout_constraintEnd_toStartOf="@+id/item_name_id"
        app:layout_constraintTop_toBottomOf="@+id/imageView3_id" />

    <ImageView
        android:id="@+id/imageView3_id"
        android:layout_width="359dp"
        android:layout_height="263dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_launcher_foreground" />


</androidx.constraintlayout.widget.ConstraintLayout>


